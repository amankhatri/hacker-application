      <a href="/">
        <%= image_tag "home.png", {:id => 'man'} %>
      </a>
    <% end %>
  </div>
  <div id="ieee_logo">
    <%= image_tag("IEEE_logo_black.jpg") %>
  </div>
</div>

<div id="main">
  <div id="sidebar-left">

  </div>

  <div id="content">

    <div id="ruby-injection">
      <%= yield %>
    </div>

  </div>

  <div id="sidebar-right">

  </div>

</div>

<div id="footer">

  <div id="footer-inner-left">
    <div id="powered_by">Powered by</div>
    <span style="padding-left: 18px;"><a href="http://vtools.ieee.org"><%= image_tag("vTools_logo_small.jpg") %></a></span>
  </div>

  <div id="footer-links">
    <a href="/">vTools.AtMeetings Home</a> |
    <a href="/about">About</a> |
    <a href="http://www.ieee.org/privacy">Privacy Policy</a> |
    <a href="http://www.ieee.org/web/publications/rights/legal.html">Terms &amp; Conditions</a> |
    <a href="http://www.ieee.org/web/aboutus/whatis/policies/p9-26.html">Nondiscrimination Policy</a>
  </div>

